/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a= a, b= a, out= aNot);
    Nand(a= b, b= b, out= bNot);
    Nand(a= a, b= bNot, out= abNot);
    Nand(a= b, b= aNot, out= baNot);
    Nand(a= abNot, b= baNot, out= out);
    
}